        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "outDir": "dist",
        "rootDir": ".",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules", "dist"]
}
